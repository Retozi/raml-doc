#%RAML 0.8
description: metadata with translations of various entities
/badge-types:
    get:
        displayName: all possible badge types
        responses:
            200:
                body:
                    application/json:
                        example: !include badge-types.json
                        schema: |
                            [
                                {
                                    id: 'integer',
                                    name: 'MultiLangString'
                                }
                            ]
/address-countries:
    get:
        displayName: List of countries that can be used in adresses
        responses:
            200:
                body:
                    application/json:
                        example: !include address-countries.json
                        schema: |
                            [
                                {
                                    id: 'integer',
                                    isoCode: 'string',
                                    name: 'MultiLangString'
                                }
                            ]


/languages:
    get:
        displayName: List of correspondence languages
        responses:
            200:
                body:
                    application/json:
                        example: !include languages.json
                        schema: |
                            [
                                {
                                    id: 'integer',
                                    name: 'MultiLangString'
                                }
                            ]

/license-plate-countries:
    get:
        displayName: List of all possible license-plates countries


/license-plate-types:
    get:
        displayName: List of possible license plate types
