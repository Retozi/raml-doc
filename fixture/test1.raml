put:
    displayName: edit the vignette field of a LicensePlate
    body:
        example: |
            {
                "vignetteNrs": ["123490334", "312483493"]
            }

        schema: |
            type: 'object'
            properties:
                vignetteNrs:
                    type: array
                    items:
                        type: string

            vignetteNrs: [ 'string' ]
    responses:
        200:
            body:
                example: |
                    {"hello": "world"}
                schema: |
                    type: object
                    properties:
                        hello: {type: string}

